    [omx:hasNumericalValueProperty nm:dos_value ;
     omx:hasUnitProperty nm:dos_unitOfMeasure]
.


nm:dos_productDosage
  owl3:dataTypePropertyChain(
                nm:productDosage
#                skos:prefLabel
                rdfs:label
                )
.

nm:dos_value
  owl3:dataTypePropertyChain(
                nm:productDosageAmount 
